using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

class prog
{

    static void factorial(int a)
    {

        for(int i=0;i<a;i++)
        {
            a = 1;
            a = a * i;
        }
    }

    static void Main(string[] args)
    {
        int n;

        Console.WriteLine("Enter no:");
        n = Convert.ToInt32(Console.ReadLine());

        factorial(n);
        Console.WriteLine(factorial);

        Console.ReadKey();
    }
}
