using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

class assig
{
    static void Main(string[] args)
    {
        int n, sq = 0, i;

        Console.Write("Enter a no:");
        n = Convert.ToInt32(Console.ReadLine());

        for (i = 1; i < 5; i++)
        {
            sq = n * i;
            Console.Write(sq);
        }
        Console.ReadKey();
    }
}
